{
  "name": "discord-tickets",
  "version": "5.0.0-alpha.0",
  "private": true,
  "description": "The most popular open-source ticket management bot for Discord.",
  "workspaces": [
    "apps/*",
	"packages/*",
	"services/*"
  ],
  "scripts": {
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s",
    "contributors:add": "all-contributors add",
    "contributors:generate": "all-contributors generate",
    "keygen": "node scripts/keygen",
    "lint": "eslint apps/**/*.js,ts --fix",
    "test": "node scripts/check-i18n",
    "__prepare": "husky install"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "lint-staged": {
    "**/*.js": [
      "npm run lint --"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/discord-tickets/bot.git"
  },
  "keywords": [
    "discord",
    "tickets",
    "bot"
  ],
  "author": "eartharoid",
  "license": "GPL-3.0-or-later",
  "bugs": {
    "url": "https://github.com/discord-tickets/bot/issues"
  },
  "homepage": "https://discordtickets.app",
  "dependencies": {},
  "devDependencies": {
    "@commitlint/cli": "^17.8.1",
    "@commitlint/config-conventional": "^19.8.0",
    "@eartharoid/eslint-rules-js": "^1.1.0",
    "@eslint/js": "^9.23.0",
    "@types/bun": "latest",
    "all-contributors-cli": "^6.26.1",
    "conventional-changelog-cli": "^5.0.0",
    "eslint": "^9.23.0",
    "globals": "^16.0.0",
    "husky": "^9.1.7",
    "lint-staged": "^15.5.0",
    "markdown-table": "^3.0.4",
    "typescript-eslint": "^8.27.0"
  },
  "peerDependencies": {
    "typescript": "^5"
  },
  "optionalDependencies": {}
}